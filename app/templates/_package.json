{
  "name": "<%= slugname %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "private": true,
  "bin": {
    "<%= slugname %>": "./bin/<%= slugname %>"
  },
  "scripts": {
    "test": "mocha -R spec"
  },
  "author": {
    "name": "<%= author %>",
    "email": "<%= authorEmail %>"<% if (authorUrl) { %>,
    "url": "<%= authorUrl %>"<% } %>
  },
  "repository": {
    "type": "git",
    "url": "<%= repoUrl %>.git"
  },
  "license": "<%= license %>",
  "bugs": "<%= bugsUrl %>",
  "dependencies": {
    <%= dependencies %>
  },
  "devDependencies": {
  }
}
